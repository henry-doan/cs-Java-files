package m1;

import java.util.Stack;

import edu.princeton.cs.algs4.Queue;
import edu.princeton.cs.algs4.StdOut;

public class Midterm1 {
	public static void main(String[] args) {
	Queue<Float> q = new Queue<Float>();
	q.enqueue((float) 2.5);
	q.enqueue((float) -5.0);
	q.enqueue((float) -7.5);
	q.enqueue((float) 0.0);
	q.enqueue((float) 5.0);
	q.enqueue((float) -2.5);


	my2(q, 3);
	}
	
	private static void my2 (Queue<Float> q, int n) {
		int size = q.size();
		for ( int i = 0; i < size; i ++) {
			Float next = q.dequeue();
			if (i < n) {
				q.enqueue(next);
			}
			
		}
		StdOut.println(q);
	}
}
