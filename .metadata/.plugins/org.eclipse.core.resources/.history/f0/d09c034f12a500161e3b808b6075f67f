package a9;

import a9.DynamicArrayAbstract;

public class DynamicArrayAddOne <T> extends DynamicArrayAbstract{
	
	@Override
	public T[] growthStrategy(int i) {
		T[] newData;
		if (count == data.length) {
			newData = (T[]) new Object[data.length + 1];
		}
		else
			newData = (T[]) data;

		for(int j = 0; j < i; j++) {
			newData[j] = (T) data[j];
		}

		for(int j = i + 1; j <= size(); j++) {
			newData[j] = (T) data[j - 1];
		}
		count++;
		return newData;
	}
	
}
